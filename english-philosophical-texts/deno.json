{
  "name": "@englishphilosophy/english-philosophical-texts",
  "version": "0.0.1",
  "license": "MIT",
  "exports": "./mod.ts",
  "tasks": {
    "dev": "PORT=3000 deno run --watch --allow-env --allow-read --allow-net bin/serve.ts",
    "start": "deno run --allow-env --allow-read --allow-net bin/serve.ts",
    "test": "deno test --allow-run --allow-net --allow-read",
    "test:watch": "deno task test --watch",
    "lint": "deno lint",
    "lint:watch": "deno task lint --watch",
    "fmt": "deno fmt --check",
    "fmt:watch": "deno task fmt --watch",
    "fmt:fix": "deno fmt"
  },
  "imports": {
    "ajv": "npm:ajv@^8.17.1"
  }
}
